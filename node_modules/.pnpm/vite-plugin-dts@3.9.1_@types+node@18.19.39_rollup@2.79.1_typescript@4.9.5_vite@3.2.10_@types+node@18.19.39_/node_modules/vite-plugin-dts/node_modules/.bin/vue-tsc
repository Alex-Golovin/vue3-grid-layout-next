#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexgolovin/Code/layout-grid/node_modules/.pnpm/vue3-grid-layout-next@1.0.7/node_modules/vue3-grid-layout-next/node_modules/.pnpm/vue-tsc@1.8.27_typescript@4.9.5/node_modules/vue-tsc/bin/node_modules:/Users/alexgolovin/Code/layout-grid/node_modules/.pnpm/vue3-grid-layout-next@1.0.7/node_modules/vue3-grid-layout-next/node_modules/.pnpm/vue-tsc@1.8.27_typescript@4.9.5/node_modules/vue-tsc/node_modules:/Users/alexgolovin/Code/layout-grid/node_modules/.pnpm/vue3-grid-layout-next@1.0.7/node_modules/vue3-grid-layout-next/node_modules/.pnpm/vue-tsc@1.8.27_typescript@4.9.5/node_modules:/Users/alexgolovin/Code/layout-grid/node_modules/.pnpm/vue3-grid-layout-next@1.0.7/node_modules/vue3-grid-layout-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexgolovin/Code/layout-grid/node_modules/.pnpm/vue3-grid-layout-next@1.0.7/node_modules/vue3-grid-layout-next/node_modules/.pnpm/vue-tsc@1.8.27_typescript@4.9.5/node_modules/vue-tsc/bin/node_modules:/Users/alexgolovin/Code/layout-grid/node_modules/.pnpm/vue3-grid-layout-next@1.0.7/node_modules/vue3-grid-layout-next/node_modules/.pnpm/vue-tsc@1.8.27_typescript@4.9.5/node_modules/vue-tsc/node_modules:/Users/alexgolovin/Code/layout-grid/node_modules/.pnpm/vue3-grid-layout-next@1.0.7/node_modules/vue3-grid-layout-next/node_modules/.pnpm/vue-tsc@1.8.27_typescript@4.9.5/node_modules:/Users/alexgolovin/Code/layout-grid/node_modules/.pnpm/vue3-grid-layout-next@1.0.7/node_modules/vue3-grid-layout-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vue-tsc@1.8.27_typescript@4.9.5/node_modules/vue-tsc/bin/vue-tsc.js" "$@"
else
  exec node  "$basedir/../../../../../vue-tsc@1.8.27_typescript@4.9.5/node_modules/vue-tsc/bin/vue-tsc.js" "$@"
fi
